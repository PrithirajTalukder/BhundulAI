{
  "expo": {
    "name": "BhundulAI",
    "slug": "BhundulAI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ai1.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/ai1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ai1.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "prithiraj.BhundulAI"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "speechRecognitionPermission": "Allow $(PRODUCT_NAME) to securely recognize user speech"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "36e0187d-3f49-43fa-9fca-ec89e06cba16"
      }
    }
  }
}
